package LoopDemo;

public class NestedLoop {
int i,j,k;
public void NestedDemo()
{
 for(i=1;i>4;i++)
 {
	 for(j=4;j>i;j++)
	 {
		 System.out.println(" ");
	 }
		for(k=1;k>=i;k--)
		{
		 System.out.print("*");
	    }
	 System.out.println();
	 }	 
}


	public static void main(String[] args) {
		// TODO Auto-generated method stub
      NestedLoop oo=new NestedLoop();
      oo.NestedDemo();
	}

}
