package LoopDemo;

public class NestedLoop {
int i,j,k;
public void NestedDemo()
{
 for(i=1;i<=4;i++)
 {
	 for(j=4;j>=1;j--)
	 {
		 if(j>i)
		 {
		 System.out.println(" ");
	 }
		 else
		 System.out.print(" *");
	    }
 }
	 System.out.println();
	 }	 
public static void main(String[] args) {
		// TODO Auto-generated method stub
      NestedLoop oo=new NestedLoop();
      oo.NestedDemo();
	}

}
