package StringDemo;

public class StringSplit {
	String str="Ritika ghai";
	String[] arr;
	public void acc()
	{
		arr=str.split(" ");
		System.out.println(arr[1]);
		
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        StringSplit oo=new StringSplit();
        oo.acc();
	}

}
